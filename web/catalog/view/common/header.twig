<header class="header_{{ theme }} box_header" ng-controller="HeaderCtrl">
  <style>
    /*#cart-fixed {*/
      /*display: none;*/
    /*}*/
    .site-navbar {
      background-color: var(--secondary);
    }
    .site-navbar:not(.fixed-top) .brand__logo {
      height: {{ pgConfig.pg_logo_height?pgConfig.pg_logo_height~'px':'54px' }};
    }
    .box_header.has_banners .site-navbar:not(.fixed-top) > div {
      z-index: 1;
      /*background: rgba(255, 255, 255, 1);*/
      width: 100%;
    }
    @media (min-width: 1280px) {
      .box_header.has_banners .site-navbar:not(.fixed-top) > div {
        /*padding: 20px !important;*/
      }
    }
    .box_header.has_banners .site-navbar .invert {
      filter: none;
    }
    .box_header.has_banners .header-top .item-top-right {
      position: relative;
      z-index: 100;
    }
    @media (min-width: 1280px) {
      .site-navbar {
        min-height: 100px;
      }
    }
    @media (max-width: 1279px) {
      .site-navbar:not(.fixed-top) .brand__logo {
        height: 32px;
        width: auto !important;
        max-width: initial;
        padding-left: 0;
      }
    }
  </style>
  <div class="header_placeholder"></div>
  <style>
    .wid_menu_top_box {
      border-top: 1px solid var(--secondary);
      border-bottom: 1px solid var(--secondary);
      background: var(--secondary);
    }
    .wid_menu_top_block {
      min-height: 90px;
    }
    .wid_menu_top_block .wid_menu_top_item:not(:first-child) {
      margin-left: -1px;
    }
    .wid_menu_top_item {
      border-top: 0;
      border-bottom: 0;
    }
    .header-top {
      background: #FEFBF4;
      position: relative;
      z-index: 2;
    }
    @media (max-width: 1200px) {
      .brand__logo {
        max-height: 60px;
      }
    }
  </style>
  <style>
    .header {
      top: 0;
      left: 0;
      width: 100%;
      z-index: 999;
    }

    .logo {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
      z-index: 999;
    }

    .header-bottom .item_menu {
      min-height: 70px;
      display: flex;
      align-items: center;
      justify-content: center;
      line-height: 20px;
      transition: 0.2s ease-in-out;
    }
    .header-bottom .item_menu:hover .menu_childs_dk, .fixed_bar .item_menu:hover .menu_childs_dk {
      opacity: 1;
      visibility: visible;
      transform: scaleY(1);
      transition: 0.2s ease-in-out;
    }
    .header-bottom .item_menu:hover > a {
      color: var(--primary);
    }

    .header-bottom {
      background: var(--secondary);
      transition: 0.3s ease-out;
    }

    .site-navbar .header-bottom {
      min-height: 100px;
      background: #ffffff;
      border-color: var(--secondary);
    }
    .site-navbar .header-bottom .item_menu {
      border-color: var(--secondary)
    }

    .sub_menu {
      opacity: 0;
      visibility: hidden;
      border-bottom: 1px solid #e5e5e5;
      position: absolute;
      width: 100%;
      top: 100%;
      left: 0;
      background: rgba(255, 255, 255, 0.7);
      backdrop-filter: blur(30px);
      -webkit-backdrop-filter: blur(30px);
      transition: 0.2s ease-in-out;
    }
    .sub_menu .item_menu {
      width: calc((100% - 120px)/8);
      border-right: 1px solid #e5e5e5;
      transition: 0.2s ease-in-out;
    }
    .sub_menu .item_menu > *{
      transition: 0.2s ease-in-out;
    }
    .sub_menu .item_menu:hover {
      background: #fff;
    }
    .sub_menu .item_menu > *:hover {
      color: var(--secondary);
    }

    .menu_childs_dk {
      opacity: 0;
      visibility: hidden;
      position: absolute;
      top: 100%;
      left: 0;
      transform: scaleY(0);
      transform-origin: top;
      min-width: 100%;
      background: rgba(255, 255, 255, 1);
      transition: 0.2s ease-out;
    }

    .menu_childs_dk a {
      transition: 0.2s ease-in-out;
    }

    .menu_childs_dk a:hover {
      color: var(--primary);
    }

    .fixed_bar .menu_childs_dk {
      background: var(--secondary);
    }

    .wid_menu_child {
      min-width: 1000px;
    }

    .header-bottom .new-date {
      color: #a5a5a5;
    }

    @media (max-width: 1279px) {
      .header {
        position: fixed;
      }
    }
  </style>
  {#fixed_bar#}
  <style>
    .fixed_bar {
      position: fixed;
      top: -105%;
      left: 0;
      width: 100%;
      background: var(--secondary);
      border-top: 1px solid var(--secondary);
      border-bottom: 1px solid var(--secondary);
      transition: 0.8s ease;
    }
    .fixed_bar > *{
      min-height: 90px;
      background: var(--secondary);
    }
    .fixed_bar .logo {
      position: relative;
    }
    .fixed_bar .brand__logo {
      max-height: 40px;
    }
    .fixed_bar .wid_menu_item:not(:last-child) {
      margin-right: -1px;
    }
    .fixed_bar.fixed-top {
      top: 0;
    }
    .fixed_bar .item_menu:hover .menu_childs_dk {
      top: calc(100% + 1px);
    }
    @media (max-width: 1440px) {
      .fixed_bar .brand__logo {
        max-height: 38px;
      }
      .fixed_bar .font-4 {
        font-size: 14px;
      }
    }
  </style>
  {#Search_form#}
  <style>
    .search_form {
      position: absolute;
      top: -500%;
      width: 100%;
      background: var(--secondary);
      transition: 0.5s ease;
      z-index: -1;
    }
    .search_form.is-open {
      top: calc(100% + 1px);
    }
    .tags span {
      padding: 5px;
      margin: 0 5px 10px 0;
      border: 1px solid var(--primary);
      color: var(--primary);
      background: transparent;
      display: inline-block;
      font-size: 13px;
      transition: 0.3s ease;
    }
    .tags span:hover {
      color: #ffffff;
      background: var(--primary);
    }
  </style>
  {#Block account, location, language#}
  <style>
    .wid_account_block, .wid_location_block, .wid_language_block {
      position: absolute;
      top: -1000%;
      width: calc(100% + 2px);
      background: var(--secondary);
      transition: 0.5s ease;
      z-index: -1;
      border-left: 1px solid var(--secondary);
      border-right: 1px solid var(--secondary);
      min-width: 130px;
    }
    .wid_account_block > a, .wid_location_block > a, .wid_language_block > a {
      display: block;
      text-align: center;
      border-bottom: 1px solid var(--secondary);
    }
    .wid_account_block > a:hover, .wid_location_block > a:hover, .wid_language_block > a:hover {
      color: #ffffff;
      background: var(--primary);
    }
    .wid_menu_top_item.active .wid_account_block, .wid_menu_item.active .wid_account_block,
    .wid_menu_top_item.active .wid_location_block, .wid_menu_item.active .wid_location_block,
    .wid_menu_top_item.active .wid_language_block, .wid_menu_item.active .wid_language_block {
      top: calc(100% + 1px);
    }
  </style>
  {#Cart popup#}
  <style>
    .cart-summary {
      display: block;
      position: fixed;
      width: 100vw;
      height: 100%;
      top: 0;
      left: 100%;
      z-index: -1;
      opacity: 0;
      transition: all 0.3s ease-out 0s;
    }
    .cart-summary.is-open {
      opacity: 1;
      left: 0;
      z-index: 9999;
    }
    .cart-summary .cart-overlay {
      position: absolute;
      width: 100%;
      height: 100%;
      top: 0;
      left: 0;
      z-index: 10;
      opacity: 0;
      visibility: hidden;
      background: rgba(0, 2, 4, 0.6);
      transition: all 0.3s ease-out 0s;
    }
    .cart-summary.is-open .cart-overlay {
      opacity: 1;
      visibility: visible;
    }
    .cart-summary.is-open .cart-popup {
      -webkit-transform: translateZ(0);
      transform: translateZ(0);
      visibility: visible;
    }
    .cart-popup {
      position: absolute;
      width: 100%;
      max-width: 630px;
      height: 100%;
      top: 0;
      right: 0;
      transition: all 0.3s ease-out 0s;
      background-color: #FFFFFF;
      overflow: hidden;
      z-index: 50;
      -webkit-transform: translate3d(100%,0,0);
      transform: translate3d(100%,0,0);
      visibility: hidden;
    }
    .cart-item .remove-cart-item {
      position: absolute;
      right: 10px;
      bottom: 50%;
      transform: translateY(50%);
    }
    .cart-item .remove-cart-item:hover .fa {
      color: rgba(48, 48, 48, 1) !important;
    }
    .cart_item_img {
      width: 100px;
      height: 100px;
    }
    .total_cart {
      padding-bottom: 50px;
      overflow: scroll;
    }
    .cart-summary .quantity button:hover {
      background-color: #0F1F2A;
      color: #FFFFFF;
    }
    .header_{{ theme }} .editor_reset p {
      margin-bottom: 0;
    }
  </style>
