      <script>
        $('.menu__item--doner').click(function() {
          $(this).toggleClass('active');
          $('.menu_mb').toggleClass('active');
          if ($('.menu_account_mb.active').length) {
            $('.menu_account_mb ').trigger('click');
          }
        });
        $('.menu_account_mb').click(function() {
          $(this).toggleClass('active');
          $('.wid_account_mb').toggleClass('active');
        });

        $('.arrow_back_mb').click(function() {
          $('.menu_account_mb ').trigger('click');
        });

        $('.menu_mb .item_menu').click(function() {
          $(this).toggleClass('text-primary');
          $(this).next($('.menu_childs')).slideToggle(200);
        })
      </script>
      <script>
        function addPaddingBottom() {
          if ($(window).width() < 1275) {
            $('body').css('padding-bottom', $('.menu_tab_mb').outerHeight() + 'px');
            // $('.menu_bar_mb').css('bottom', $('.menu_tab_mb').outerHeight() + 'px');
            // $('.menu_tab_mb .menu_childs').css('padding-bottom', $('.menu_tab_mb').outerHeight() + 'px');
          } else {
            $('body').css('padding-bottom', '');
          }
        }
        $(document).ready(function() {
          $('.menu_tab_item').click(function() {
            $('.menu_tab_item').not(this).removeClass('active');
            $(this).toggleClass('active');
            if ($('.menu_tab_item').hasClass('active')) {
              $('body').css('overflow', 'hidden');
            } else {
              $('body').css('overflow', '');
            }
          });
          addPaddingBottom();
          $(window).resize(addPaddingBottom);
        });

        function toggleActive(e) {
          if ($(e).parent('.active').length) {
            $(e).parent().toggleClass('active');
          } else {
            $('.wid_menu_top_item.active, .wid_menu_item.active').removeClass('active');
            $(e).parent().toggleClass('active');
          }
        }
      </script>
