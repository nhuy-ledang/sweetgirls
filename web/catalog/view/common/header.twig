    </div>{% endif %}
  </div>
  <script>
    var noti_height = $('.wid_notify').outerHeight();
    var header_height = $('.header').outerHeight();
    function set_noti() {
      if ($(window).width() < 1275) {
        {% if global.config_comment %}
          $('.header').css('top', noti_height + 'px').css('transition', '0.5s ease');
        {% endif %}
        $('#main-content, main').css('padding-top', noti_height + header_height + 'px').css('transition', '0.5s ease');
      }
    }
    $(window).resize(function() {
      set_noti()
    });
    // Set width menu child
    $(document).ready(function() {
      $('.wid_menu_child').css('width', $('.get_width_menu').outerWidth());
      set_noti();
      fixed_header();
    });

    // Fixed header
    let header_offsettop = document.querySelector('.site-navbar');
    let h_menu = header_offsettop.offsetHeight + header_offsettop.offsetTop;
    let prevScroll = window.pageYOffset;

    function fixed_header() {
      $(window).scroll(function() {
        var currentScroll = window.pageYOffset;
        var scroll = $(window).scrollTop();
        if (scroll > h_menu && currentScroll < prevScroll) {
          $('.fixed_bar').addClass('fixed-top');
        } else if (scroll <= h_menu || currentScroll >= prevScroll) {
          $('.fixed_bar').removeClass('fixed-top');
        }
        prevScroll = currentScroll;

        // Header mobile
        if ($(window).width() < 1275) {
          if (scroll > 0) {
            var header_height = $('.header').outerHeight();

            $('.header_mb').addClass('fixed-top');
            console.log(header_height);
            
            // Filter category mobile
            $('.sticky_mb').css('top', noti_height + header_height + 'px');
          } else {
            $('.header_mb').removeClass('fixed-top');
          }
        }
      });
    }

    function toggleActive(e) {
      if ($(e).parent('.active').length) {
        $(e).parent().toggleClass('active');
      } else {
        $('.wid_menu_top_item.active, .wid_menu_item.active').removeClass('active');
        $(e).parent().toggleClass('active');
      }
    }
  </script>
