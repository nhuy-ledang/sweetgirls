<!DOCTYPE html>
<html dir="{{ direction }}" lang="{{ lang }}" ng-app="App" ng-controller="AppCtrl">
<head>
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>{{ title }}</title>
<base href="{{ base }}">
{% if description %}
<meta name="description" content="{{ description }}">{% endif %}
{% if keywords %}
<meta name="keywords" content="{{ keywords }}">{% endif %}
{% for link in links %}{% if link['rel']=='canonical' %}{% set canonical=link['href'] %}{% endif %}
<link href="{{ link.href }}" rel="{{ link.rel }}">{% endfor %}
{% for style in styles %}
<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}">{% endfor %}
{#<!-- Social Meta Tags -->#}
<meta property="og:title" name="title" content="{{ title }}">
{% if description %}
<meta property="og:description" name="description" content="{{ description }}">{% endif %}
{% if facebook_id %}<meta property="fb:app_id" content="{{ facebook_id }}">{% endif %}
<meta property="og:site_name" content="{{ name }}">
{% if canonical %}
<meta property="og:url" content="{{ canonical }}">{% endif %}
{% if image %}
<meta property="og:image" content="{{ image }}">
<meta property="og:image:url" content="{{ image }}">
{% if image_alt %}
<meta property="og:image:alt" content="{{ image_alt }}">{% endif %}
{% endif %}
{#<!-- End Social Meta Tags -->#}
{#<!-- Other meta tags -->#}
{% for item in metaTags %}
<meta property="{{ item['property'] }}" content="{{ item['content'] }}">{% endfor %}
{% if global.config_scripts %}{{ global.config_scripts }}{% endif %}
{% if canonical %}
<meta property="al:web:url" content="{{ canonical }}">{% endif %}
<link rel="stylesheet" href="/assets/js/swiper/swiper-bundle.min.css"/>
<link rel="stylesheet" href="/assets/js/animate.min.css">
<link rel="stylesheet" href="/assets/css/loader.css">
{% if style_page %}
<link rel="stylesheet" href="/assets/css/styles{{ style_page }}.css">{% endif %}
{% for cssCode in cssCodes %}
<style>{{ cssCode }}</style>{% endfor %}
<script>window.user={{ userData|json_encode }};window.settings={{ initData|json_encode }};window.labels={{ labels|json_encode }};</script>
{#<!-- jQuery (necessary for JavaScript plugins) -->#}
{#<script src="//ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>#}
<script type="text/javascript" src="/content/assets/js/jquery-1.12.4.min.js"></script>
<script type="text/javascript" src="/assets/js/slick-carousel/slick/slick.min.js"></script>
<script type="text/javascript" src="/assets/js/lightgallery-all.min.js"></script>
<script type="text/javascript" src="/assets/js/swiper/swiper-bundle.min.js"></script>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

{#<script type="text/javascript" src="/assets/js/gmap.js"></script>#}
{#{% if global.config_google_api_key %}<script type="text/javascript" src="https://maps.google.com/maps/api/js?key={{ global.config_google_api_key }}"></script>{% endif %}#}
{% for script in scripts %}
<script type="text/javascript" src="{{ script }}"></script>{% endfor %}
