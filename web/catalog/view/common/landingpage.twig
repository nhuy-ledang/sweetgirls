    <div class="px-5 py-4 mb-1 faq-tab{{ loop.index }}" style="box-shadow: 0px 2px 3px #00000029;">
      <div class="row no-gutters">
        <div class="col-11 h4 text-primary font-weight-bolder d-flex align-items-center">Lorem ipsum dolor sit amet, consectetur.</div>
        <div class="col-1 d-flex justify-content-center"><a href="" target="{{ loop.index }}" class="plus-more-big"></a></div>
        <div class="col-11 h5 lh-30 faq-content{{ loop.index }}" style="display: none">
          <div>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Aspernatur atque consequatur dolorum facere harum id, illum impedit laudantium magni minus nam nostrum possimus quaerat reprehenderit sit temporibus vel vero voluptatibus.</div>
        </div>
      </div>
    </div>
  {% endfor %}

  <style>
    .faq-tab.active{
      box-shadow: 5px 5px 10px #00000029 !important;
    }
    .plus-more-big{
      display: block;
      width: 40px;
      height: 40px;
      position: relative;
      text-decoration: none;
      background-color: transparent;
      color: var(--primary);
    }
    .plus-more-big:after, .plus-more-big:before {
      content: '';
      position: absolute;
      background: var(--primary);
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%) rotate(0deg);
      z-index: 1;
    }
    .plus-more-big:before{
      height: 4px;
      width: 25px;
      border-radius: 50px;
    }
    .plus-more-big:after{
      width: 4px;
      height: 25px;
      border-radius: 50px;
    }

    .plus-more-big.active:after{
      opacity: 0%;
    }
  </style>

  <script>
    $('.plus-more-big').click(function(){
      var index = $(this).attr("target");
      if($('.faq-tab'+ index).hasClass("active")){
        $('.faq-content'+ index).css({"display": "none"});
        $('.faq-tab'+ index).removeClass("active");
        $(this).removeClass("active");
      }else{
        $('.faq-content'+ index).css({"display": "block"});
        $('.faq-tab'+ index).addClass("active");
        $(this).addClass("active");
      }
    });
  </script>
</div>


