<div class="container my-12">
  <div class="h1 section-title" data-tile=""><span>Lorem ipsum</span></div>
  <div class=" h4 section-desc"  >Lorem ipsum dolor sit amet, consectetur adipisicing elit. Cum dignissimos eum possimus quisquam vero? Adipisci fugiat praesentium soluta unde voluptatum.</div>

  <div class="row mt-6 mt-md-10 mb-md-10 improve slider-history">
    {% for item in data %}
      <div class="col-12 d-flex flex-wrap">
        <div class="col-12 col-md-6 pr-md-10 pr-xl-13">
          <div class="embed-responsive embed-responsive-16by9" style="background: url('assets/demo/home-new-3.png') no-repeat center / cover"></div>
        </div>
        <div class="col-12 col-md-6 dev-desc mt-10 mt-md-0">
          <div class="h3 font-weight-bolder text-primary mb-xl-10">Lorem ipsum dolor sit amet.</div>
          <div class="h4 lh-30 mb-10 mb-xl-0 mt-5 mt-md-0">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ab nesciunt officiis, optio pariatur quae velit.{{ loop.index }}</div>
        </div>
      </div>
    {% endfor %}
  </div>

  <div class="thumb-slider-history position-relative px-0" style="margin-top: -7px">
    <div class="container px-0">
      <div class="thumb-slider position-relative" style="opacity: 0;transition: all 0.5s ease-in-out 1s;">
        {% for item in data %}
          <div class="item-thumb-slider">
            <div class="h0 font-weight-bolder text-center">0{{ loop.index }}</div>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>

  <style>
    .thumb-slider-history .item-thumb-slider{
      padding-top: 2rem;
      position: relative;
    }
    .thumb-slider-history .item-thumb-slider:before{
      content: '';
      position: absolute;
      width: 1.25rem;
      height: 1.25rem;
      border-radius: 50%;
      background: #e8e8e8;
      top: 0;
      left: 50%;
      transform: translateX(-50%);
    }
    .thumb-slider-history .item-thumb-slider.slick-current .h1{
      color: var(--primary);
    }
    .thumb-slider-history .item-thumb-slider.slick-current:before{
      background: var(--primary);
    }
  </style>

  <script>
    $(window).load(function () {
      $('.slider-history').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        arrows: false,
        fade: true,
        asNavFor: '.thumb-slider'
      });
      $('.thumb-slider').slick({
        slidesToShow: 6,
        slidesToScroll: 1,
        asNavFor: '.slider-history',
        dots: false,
        arrows: false,
